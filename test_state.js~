    create: function(){
        game.stage.backgroundColor = "#ffff00"
        this.ellipse_center_x = game_w *.5 * Math.random() + game_w * .25;
        this.ellipse_center_y = game_h *.5 * Math.random() + game_h * .25;
        var graphics = game.add.graphics(
            this.ellipse_center_x, this.ellipse_center_y);
        graphics.beginFill(0xFF0000);
        graphics.drawCircle(0, 0, 10);
        graphics.endFill();
        this.sample_text = [];
        this.text_paths = [];
        this.pos_counter = 0;
        var tau = Math.PI / 2;
        for (var i = 1; i <= 4; i++){
            var anxiety = 25 * i;
            var text = game.add.text(
                this.ellipse_center_x + Math.cos(tau * i) * anxiety * 2,
                this.ellipse_center_y + Math.sin(tau * i) * anxiety * 2,
                "Ayy lmao");
            text.font = "Press Start 2P";
            text.fontSize = (100 - anxiety) * .20 + 9;
            this.sample_text.push(text);
            // balance k, l and g later
            var k= 2;
            var l = 2;
            var g = 1;
            var a = anxiety;
            var b = Math.sqrt(a * a * a / 100);
            var e_x = Math.sqrt(a * a - b * b) * RandomInt(-1, 1);
